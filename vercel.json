{
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 300,
      "memory": 2048
    },
    "app/api/generate/route.ts": {
      "maxDuration": 900,
      "memory": 2048
    },
    "app/api/upload/route.ts": {
      "maxDuration": 30,
      "memory": 2048
    }
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, x-model-id"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
